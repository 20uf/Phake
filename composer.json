{
    "name": "phake/phake",
    "type": "library",
    "description": "The Phake mock testing library",
    "keywords": ["testing", "mock"],
    "homepage": "https://github.com/mlively/Phake",
    "license": "BSD",
    "authors": [
        {
            "name": "Mike Lively",
            "email": "m@digitalsandwich.com"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "ext-soap":"*",
        "doctrine/common": "2.3.*",
        "phpunit/phpunit": "3.7.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "include-path": ["src"],
    "suggest": {
        "doctrine/common": ">=2.3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
